# API Configuration
# For local development:
REACT_APP_API_URL=http://localhost:8081
# For production (set in Vercel environment variables):
# REACT_APP_API_URL=https://your-backend-domain.onrender.com

REACT_APP_AI_API_URL=https://ai-court-ai.onrender.com/api

# WebSocket Configuration
# For local development:
REACT_APP_WS_URL=http://localhost:8081/ws
# For production (set in Vercel environment variables):
# REACT_APP_WS_URL=wss://your-backend-domain.onrender.com/ws

# Application Configuration
REACT_APP_NAME=AI Courtroom
REACT_APP_VERSION=1.0.0

# Features Toggle
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_VIDEO_CALLS=false

# Debug Mode (set to false in production)
REACT_APP_DEBUG=true

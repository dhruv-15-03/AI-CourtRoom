# Production Environment Variables for Vercel

# Disable treating warnings as errors
CI=false
DISABLE_ESLINT_PLUGIN=true
GENERATE_SOURCEMAP=false

# API Configuration - Update these with your production URLs
REACT_APP_API_URL=https://your-backend-domain.com
REACT_APP_AI_API_URL=https://ai-court-ai.onrender.com/api

# WebSocket Configuration
REACT_APP_WS_URL=wss://your-backend-domain.com/ws

# Application Configuration
REACT_APP_NAME=AI Courtroom
REACT_APP_VERSION=1.0.0

# Features Toggle
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_VIDEO_CALLS=false

REACT_APP_DEBUG=false
